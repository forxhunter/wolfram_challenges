Cell[BoxData[
 RowBox[{
  RowBox[{"BubbleSortSteps", "[", 
   RowBox[{"list", ":", 
    RowBox[{"{", "__Integer", "}"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"templ", "=", "list"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"outlist", "=", 
      RowBox[{"{", "templ", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"l", "=", 
      RowBox[{"Length", "[", "templ", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<", "l"}], ",", 
       RowBox[{"i", "++"}], ",", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"j", "=", "1"}], ",", 
         RowBox[{"j", "<", "l"}], ",", 
         RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"templ", "[", 
              RowBox[{"[", "j", "]"}], "]"}], ">", 
             RowBox[{"templ", "[", 
              RowBox[{"[", 
               RowBox[{"j", "+", "1"}], "]"}], "]"}]}], ",", 
            RowBox[{"templ", "=", 
             RowBox[{"ReplacePart", "[", 
              RowBox[{"templ", ",", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"j", "\[Rule]", 
                  RowBox[{"templ", "[", 
                   RowBox[{"[", 
                    RowBox[{"j", "+", "1"}], "]"}], "]"}]}], ",", 
                 RowBox[{
                  RowBox[{"j", "+", "1"}], "\[Rule]", 
                  RowBox[{"templ", "[", 
                   RowBox[{"[", "j", "]"}], "]"}]}]}], "}"}]}], "]"}]}]}], 
           "]"}], ";", 
          RowBox[{"outlist", "=", 
           RowBox[{"Append", "[", 
            RowBox[{"outlist", ",", "templ"}], "]"}]}]}]}], "]"}]}], "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"outlist", "=", 
      RowBox[{"Drop", "[", 
       RowBox[{"outlist", ",", 
        RowBox[{"-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     "outlist"}]}], "]"}]}]], "Input",
